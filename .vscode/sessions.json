{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v9/terminal-keeper.json",
  "theme": "default",
  "active": "default",
  "activateOnStartup": false,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "root",
        "commands": [
          "clear"
        ],
        "autoExecuteCommands": true,
        "icon": "home"
      },
      {
        "name": "ngx-kit-build",
        "commands": [
          "cd projects/ngx-kit",
          "clear",
          "ng build --watch"
        ],
        "autoExecuteCommands": true,
        "color": "terminal.ansiRed",
        "icon": "eye"
      },
      {
        "name": "ngx-kit",
        "commands": [
          "cd projects/ngx-kit/src/lib",
          "clear"
        ],
        "autoExecuteCommands": true
      },
      {
        "name": "testground-server",
        "commands": [
          "cd projects/testground",
          "clear",
          "make dev"
        ],
        "autoExecuteCommands": true,
        "color": "terminal.ansiRed",
        "icon": "server"
      },
      {
        "name": "testground",
        "commands": [
          "cd projects/testground/src/app",
          "clear"
        ],
        "autoExecuteCommands": true
      }
    ]
  }
}